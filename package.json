{
  "name": "RN",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl",
    "android-release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "android-build-apk": "cd android && ./gradlew assembleRelease",
    "clear-cache": "watchman watch-del-all && rm -f yarn.lock && rm -rf node_modules && yarn && yarn start --reset-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.13.4",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "5.9.3",
    "@react-navigation/stack": "5.14.2",
    "@welldone-software/why-did-you-render": "^6.0.5",
    "i18next": "19.8.9",
    "i18next-http-backend": "1.1.0",
    "immutable": "4.0.0-rc.12",
    "lodash": "4.17.20",
    "moment": "2.29.1",
    "normalizr": "3.6.1",
    "react": "16.13.1",
    "react-i18next": "11.8.7",
    "react-native": "0.63.4",
    "react-native-code-input": "1.0.6",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-popover-view": "3.1.1",
    "react-native-reanimated": "2.0.0-rc.3",
    "react-native-snap-carousel": "3.9.1",
    "react-native-status-bar-height": "2.6.0",
    "react-native-storybook-loader": "2.0.2",
    "react-native-svg": "12.1.0",
    "react-native-svg-transformer": "0.14.3",
    "react-native-vector-icons": "8.1.0",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-form": "8.3.7",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-query": "3.4.2",
    "redux-query-interface-superagent": "3.4.2",
    "redux-query-react": "3.4.2",
    "rn-range-slider": "2.0.4",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.1",
    "@babel/runtime": "7.13.7",
    "@react-native-community/eslint-config": "2.0.0",
    "@storybook/addon-actions": "5.3",
    "@storybook/addon-contexts": "5.3.21",
    "@storybook/addon-knobs": "5.3",
    "@storybook/addon-ondevice-actions": "5.3.23",
    "@storybook/addon-ondevice-knobs": "5.3.25",
    "@storybook/addon-storyshots": "^6.1.20",
    "@storybook/addon-storyshots-puppeteer": "^6.1.20",
    "@storybook/react-native": "5.3.25",
    "@storybook/react-native-server": "5.3.23",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.20.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.65.1",
    "prop-types": "15.7.2",
    "puppeteer": "^8.0.0",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/react-native.js"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
